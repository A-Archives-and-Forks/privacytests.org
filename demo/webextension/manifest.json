{
  "manifest_version": 2,
  "name": "resist-fingerprinting-js-webextension",
  "version": "0.01",
  "description": "Demonstrates how to use resist-fingerprinting-js with webextensions",

  "icons": {
    "48": "icons/rfp-we-48.png"
  },

  "web_accessible_resources": ["*"],

  "content_scripts": [
    {
      "all_frames": true,
      "run_at": "document_start",
      "matches": ["<all_urls>"],
      "js": ["injector.js"]
    }
  ]
}
